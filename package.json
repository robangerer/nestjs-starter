{
  "name": "nestjs-starter",
  "version": "0.0.28",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:party-service": "nest build party-service",
    "build:user-replicator": "nest build user-replicator",
    "start": "nest start",
    "start:user-replicator": "nest start user-replicator",
    "start:win": "docker-compose -f docker-compose.dev.yml up -V -d --force-recreate && SET LOCAL_RUN=true&& SET NODE_ENV=test&& nest start party-service --watch --debug",
    "start:lin": "docker-compose -f docker-compose.dev.yml up -V -d --force-recreate && export LOCAL_RUN=true&& export NODE_ENV=test&& nest start party-service --watch --debug",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest && npm run validate-api-specs",
    "test:unit": "jest --selectProjects unit-tests",
    "test:int": "jest --selectProjects integration-tests",
    "test:int-admin": "jest --selectProjects integration-tests --roots=apps/party-service/test/admin-bff",
    "test:int-sfs": "jest --selectProjects integration-tests --roots=apps/party-service/test/self-service",
    "test:int-other": "jest --selectProjects integration-tests --roots=apps/party-service/test/idp-connector --roots=apps/party-service/test/internal-api --roots=apps/party-service/test/pubsub --roots=apps/party-service/test/external-api",
    "test:acc": "jest --selectProjects acceptance-tests",
    "auto-release": "auto shipit",
    "auto-release-dry-run": "auto shipit --dry-run --quiet",
    "auto-comment-release-notes": "auto comment --edit --message \"$(auto changelog --dry-run)\"",
    "helm-bump-version": "helm-version helm/party-service",
    "country-code-migration": "node db/scripts/country-code-migration.mjs",
    "validate-api-specs": "oas-validate -v helm/party-service/api-doc/",
    "role-mapping-user-association-migration": "node db/scripts/role-mapping-user-association-migration.mjs",
    "import-vat-id-from-carriers": "node db/scripts/import-vat-id-from-carriers.mjs",
    "import-address-from-gpsa-carriers": "node db/scripts/import-address-from-gpsa-carriers.mjs",
    "customer-vat-validation": "node db/scripts/customer-vat-validation.mjs",
    "customer-set-vatstack-vat": "node db/scripts/customer-create-oi-oiv.mjs"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.19.0",
    "@google-cloud/secret-manager": "^3.10.1",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/swagger": "^6.0.2",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "auth0": "^2.40.0",
    "avro-js": "^1.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.7",
    "express-openapi-validator": "^4.13.2",
    "google-auth-library": "^8.0.2",
    "i18n-iso-countries": "^7.4.0",
    "iso8601-duration": "^1.3.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^1.4.0",
    "node-mailjet": "^3.3.4",
    "pg": "^8.7.1",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.1",
    "source-map-support": "^0.5.20",
    "typeorm": "^0.2.37",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@auto-it/jira": "^10.32.0",
    "@auto-it/omit-commits": "^10.32.0",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.10.9",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "auto": "^10.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "helm-version-js": "^1.0.0",
    "husky": "^4.3.8",
    "jest": "^27.2.1",
    "lint-staged": "^10.5.4",
    "pino-pretty": "7.3.0",
    "prettier": "^2.5.0",
    "supertest": "^6.1.6",
    "swagger2openapi": "^7.0.8",
    "testcontainers": "^7.20.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md,json,yaml}": "prettier --write"
  }
}
